   "source": [
    "weight = input('Please enter your weight: ')\n",
    "height = input('Please enter your height: ')\n",
    "\n",
    "def imc_calculation(w,h):\n",
    "    height = float(h)\n",
    "    weight = int(w)\n",
    "    return weight / height\n",
    "\n",
    "imc = imc_calculation(weight, height)\n",
    "\n",
    "result = 'Your IMC is: %s' % (round(imc,2))\n",
    "\n",
    "print(result)"
   ]
